{
  "extends": ["eslint:recommended", "google"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["html"],
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "comma-dangle": ["error", "never"]
  }
}
