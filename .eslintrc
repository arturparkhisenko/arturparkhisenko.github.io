{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "serviceworker": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-console": 0,
    "no-param-reassign": 0,
    "require-jsdoc": "off"
  }
}
