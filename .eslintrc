{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": [
    "html",
    "import"
  ],
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "comma-dangle": ["error", "never"]
  }
}
